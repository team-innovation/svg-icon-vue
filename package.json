{
  "name": "svg-icon-vue",
  "version": "1.0.0",
  "description": "SVG Icons as a Vue Component",
  "author": {
    "name": "Michael Dodge",
    "email": "michael.dodge@vivint.com"
  },
  "homepage": "https://github.com/team-innovation/svg-icon-vue/",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/team-innovation/svg-icon-vue.git"
  },
  "main": "dist/svg-icon.umd.js",
  "module": "dist/svg-icon.esm.js",
  "unpkg": "dist/svg-icon.min.js",
  "browser": {
    "./sfc": "src/svg-icon.vue"
  },
  "files": [
    "dist/*",
    "src/*"
  ],
  "scripts": {
    "build": "cross-env NODE_ENV=production rollup --config build/rollup.config.js",
    "build:umd": "cross-env NODE_ENV=production rollup --config build/rollup.config.js --format umd",
    "build:es": "cross-env NODE_ENV=production rollup --config build/rollup.config.js --format es",
    "build:unpkg": "cross-env NODE_ENV=production rollup --config build/rollup.config.js --format iife",
    "lint": "eslint \"**/*.{js,vue}\""
  },
  "dependencies": {},
  "devDependencies": {
    "@vue/eslint-config-airbnb": "^4.0.0",
    "babel-eslint": "^10.0.1",
    "cross-env": "^5.2.0",
    "eslint": "^5.14.1",
    "eslint-config-airbnb-base": "^13.1.0",
    "eslint-plugin-import": "^2.16.0",
    "eslint-plugin-vue": "^5.2.2",
    "lz-string": "^1.4.4",
    "minimist": "^1.2.0",
    "raw-loader": "^1.0.0",
    "rollup": "^1.1.2",
    "rollup-plugin-buble": "^0.19.6",
    "rollup-plugin-commonjs": "^9.2.0",
    "rollup-plugin-node-resolve": "^4.0.1",
    "rollup-plugin-replace": "^2.1.0",
    "rollup-plugin-require-context": "0.0.2",
    "rollup-plugin-string": "^3.0.0",
    "rollup-plugin-svgo": "^1.0.2",
    "rollup-plugin-terser": "^4.0.4",
    "rollup-plugin-vue": "^4.6.2",
    "svgo": "^1.2.0",
    "svgo-loader": "^2.2.0",
    "vue": "^2.5.22",
    "vue-template-compiler": "^2.5.22"
  }
}
